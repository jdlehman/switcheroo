{
  "name": "switcheroo",
  "version": "2.0.2",
  "description": "React component that switches between components based on route",
  "main": "dist/switcheroo.min.js",
  "scripts": {
    "build": "$(npm bin)/webpack",
    "lint:src": "$(npm bin)/eslint ./src",
    "lint:test": "$(npm bin)/eslint ./test",
    "lint": "npm run lint:src && npm run lint:test",
    "test": "NODE_PATH=$NODE_PATH:$PWD/src $(npm bin)/mocha -R dot --compilers js:babel-core/register --require ./test/setup.js --recursive",
    "clean": "rm -rf dist",
    "build-examples": "$(npm bin)/babel-node ./examples/buildAll.js",
    "deploy-site": "git subtree push --prefix examples origin gh-pages",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdlehman/switcheroo.git"
  },
  "bugs": {
    "url": "https://github.com/jdlehman/switcheroo/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "routing",
    "javascript"
  ],
  "files": [
    "dist"
  ],
  "author": "Jonathan Lehman <jonathan.lehman91@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.7.5",
    "babel-core": "6.7.5",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "6.6.1",
    "chai": "3.5.0",
    "eslint": "2.7.0",
    "eslint-config-customink": "2.1.1",
    "eslint-plugin-react": "4.3.0",
    "jsdom": "8.3.0",
    "mocha": "2.4.5",
    "react": "15.0.0",
    "react-dom": "15.0.0",
    "sinon": "1.17.3",
    "webpack": "1.12.14"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  }
}
