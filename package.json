{
  "name": "switcheroo",
  "version": "3.1.0",
  "description": "React component that switches between components based on route",
  "main": "dist/switcheroo.umd.min.js",
  "jsnext:main": "dist/switcheroo.es-modules.js",
  "scripts": {
    "build": "npm run clean && $(npm bin)/rollup -c && npm run minify",
    "minify": "npm run minify-browser && npm run minify-umd",
    "minify-browser": "$(npm bin)/uglifyjs dist/switcheroo.browser.js -cm > dist/switcheroo.browser.min.js",
    "minify-umd": "$(npm bin)/uglifyjs dist/switcheroo.umd.js -cm > dist/switcheroo.umd.min.js",
    "lint:src": "$(npm bin)/eslint ./src",
    "lint:test": "$(npm bin)/eslint ./test",
    "lint": "npm run lint:src && npm run lint:test",
    "test": "NODE_PATH=$NODE_PATH:$PWD/src $(npm bin)/mocha -R dot --compilers js:babel-register --require ./test/setup.js --recursive",
    "clean": "rm -rf dist",
    "build-examples": "$(npm bin)/babel-node ./examples/buildAll.js",
    "deploy-site": "git subtree push --prefix examples origin gh-pages",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jdlehman/switcheroo.git"
  },
  "bugs": {
    "url": "https://github.com/jdlehman/switcheroo/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "routing",
    "javascript"
  ],
  "files": [
    "dist"
  ],
  "author": "Jonathan Lehman <jonathan.lehman91@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-es2015-rollup": "1.2.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "6.11.6",
    "chai": "3.5.0",
    "eslint": "3.3.1",
    "eslint-config-customink": "2.1.1",
    "eslint-plugin-react": "6.1.2",
    "jsdom": "9.4.2",
    "mocha": "3.0.2",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "rollup": "0.34.10",
    "rollup-plugin-babel": "2.6.1",
    "rollup-plugin-replace": "1.1.1",
    "sinon": "1.17.5",
    "uglify-js": "2.7.3"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  }
}
